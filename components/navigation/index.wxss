.top-navigation {
    width: 100%;
    height: 130rpx;
    position: relative;
    background: #fff;
    color: #000000;
    align-items: center;
    display: flex;
    flex-direction: row;
}

.top-navigation .title {
    text-align: center;
    width: 100%;
    color: #000000;
    height: 100rpx;
    line-height: 140rpx;
    font-size: 30rpx;
    vertical-align: middle;
    line-height: 140rpx;
}

.navigation-fixed {
    position: fixed;
    top: 0;
    z-index: 3;
}

.back {
    position: absolute;
    left: 30rpx;
    top: 50%;
    color: #000000;
    height: 20rpx;
    width: 20rpx;
    transform: rotate(-135deg) translateY(-50%);
    border-top: 1rpx solid #000000;
    border-right: 1rpx solid #000000;
}